# This file is for unifying the coding style for different editors and IDEs
# For vscode support install EditorConfig for VS Code plugin
# editorconfig.org

root = true

[*.{js,jsx,ts,tsx,json}]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[**/vendor/**]
ignore = true

[**/node_modules/**]
ignore = true
