{
  "compilerOptions": {
    "strict": true,
    "allowJs": false,
    "checkJs": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": false,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "NodeNext",
    "module": "NodeNext",
    "target": "ES2021",
    "lib": [
      "ES2021",
      "DOM"
    ],
    "skipLibCheck": true,
    "noEmitOnError": true,
    "types": [
      "vitest/globals"
    ],
    "resolveJsonModule": true,
    "useUnknownInCatchVariables": false
  }
}